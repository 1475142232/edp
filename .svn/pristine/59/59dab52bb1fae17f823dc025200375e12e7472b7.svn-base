 	            	             async: true,
 	            	             dataType: "json",
 	            	             data:{"id":this_a.id,"treeState":"00"},
 	            	             success: function(data){
 	            	            	 //刷新列表-调用加载列表方法
 	            	            	_this.getPrototypeList(_productId);
 	                             }
 	                   	 	});
 	            		});
 	            		//编辑修改
 	            		$(".togglelist").on("click",function(){
 	            			var this_b = this;
 	            			$.ajax({
 	            	             type: "POST",
 	            	             url: "TreeController/findTreeNodeById.action",
