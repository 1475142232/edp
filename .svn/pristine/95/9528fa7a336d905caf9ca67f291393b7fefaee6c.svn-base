  <select id="findTaskInfoByDate" resultMap="BaseResultMap">
      select   *   from   task_info   
      where  (Date(str_to_date(task_done_time, '%m/%d/%Y')) > Date(str_to_date(#{date}, '%m/%d/%Y')) 
      or Date(str_to_date(task_done_time, '%m/%d/%Y')) = Date(str_to_date(#{date}, '%m/%d/%Y'))) 
      and task_state in (${state})
      and task_owner = #{user_id}
  </select>
