package com.edp.serviceI.exception;


import com.edp.common.exception.EbizException;

/**
 * ClassName: EbizException <br/>
 * Function: 自定义检查异常 <br/>
 * date: 2015年3月2日 下午1:14:03 <br/>
 *
 * @author st-sb3915
 * @version 
 * @since JDK 1.6
 */
public class ZgbException extends EbizException {

    private static final long serialVersionUID = -3760309764397856350L;

    public ZgbException(Throwable e) {
        super("", e.getMessage());
    }

    public ZgbException(String msg) {
        super("", msg);
    }

    public ZgbException(String msg, Throwable e) {
        super("", msg);
    }

    public ZgbException(String code, String message) {
        super(code, message);
    }

    public ZgbException(String code, String message, Throwable cause) {
        super(code, message, cause);
    }
}
