@charset "utf-8";
/*=========================Reset_start==========================*/
body,h1,h2,h3,h4,h5,h6,div,p,dl,dt,dd,ol,ul,li,form,table,th,td,a,
img,span,strong,var,em,input,textarea,select,option{margin: 0; padding: 0;}
html,body{font-family:"宋体","微软雅黑",Arail,Tabhoma; font-size: 12px;
	text-align: left;}
ul,ol{list-style: none;}
img{border: 0;}
input,select,textarea{outline:0;}
table{border-collapse: collapse; border-spacing: 0;}
th,strong,var,em{font-weight: normal; font-style: normal;}
a{text-decoration: none;}
a:link,a:visited,a:hover,a:active{text-decoration:none;}
a:hover {text-decoration: none;}
/*==========================Reset_End===========================*/
/*项目列表*/
.dropdown-toggle{font-size:20px;color:#CC9966 !important;font-weight: bold;color:#996666 !important}
.nav-color-zhu{font-size:16px;color:#CC9966 !important;font-weight: bold;}
.content-box-zhu{width:100%;min-height:500px;padding:40px 40px;border-radius:8px;background:#fff;margin:0 auto;-webkit-box-shadow:3px 3px 3px #CCCCCC;  -moz-box-shadow:3px 3px 3px #CCCCCC;box-shadow:3px 3px 3px #CCCCCC; }
#myProject{width:220px;height:150px;margin:20px;display:inline-block;background-color:rgba(249, 249, 249, 1);border:1px solid #eee;border-radius:4px;background-image: url(../img/car.png); background-repeat:no-repeat;background-position:85px 30px;position:relative;}
#addProject{width:220px;height:150px;font-size:90px;text-decoration: none;font-weight:bolder;margin:20px;display:inline-block;background-color:rgba(249, 249, 249, 1);text-align:center;line-height:150px;vertical-align:top;}
#myProject>dl{width:65px;height:100px;display:none;text-align: center;background-image: url(../img/list.png);background-repeat:no-repeat;position:absolute;right:0px;top:30px;}
#myProject>dl>dd{width:60px;height:auto;display:block;padding-top:20px;}
.books{z-index:2;width:20px;height:20px;display:block;background-image:url(../img/books.png); background-repeat:no-repeat;background-position:center;float:right;margin:10px 20px;}			
.wode{width:auto;height:auto;margin-top:90px;text-align:center;display:block;font-size:20px;color:#333;
		overflow:hidden;word-break:keep-all;white-space:nowrap;text-overflow:ellipsis;}
.miaoshu{width:auto;height:auto;text-align:center;display:block;font-size:14px;color:#666;
		overflow:hidden;word-break:keep-all;white-space:nowrap;text-overflow:ellipsis;}
.product{
	width: 100%;
	height: 55px;
	line-height: 50px;
	border-bottom: 2px solid #9BCE37;
}

.productName{
	float: left;
    font-size: 22px;
}

.back{
	width: 55px;
    height: 45px;
    font-size: 16px;
    background: url(../img/u211.png) no-repeat;
    cursor: pointer;
    background-position: center;
    float: right;
}